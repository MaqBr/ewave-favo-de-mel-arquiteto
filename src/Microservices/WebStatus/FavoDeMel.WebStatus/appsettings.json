{
  "BASE_PATH": "https://localhost:5012",
  "MEMORY-LIMIT": 1024,
  "BankingDbConnection": "Server=(localdb)\\MSSQLLocalDB;Database=BankingDb;Integrated Security=true;MultipleActiveResultSets=true",
  "TransferDbConnection": "Server=(localdb)\\MSSQLLocalDB;Database=TransferDb;Integrated Security=true;MultipleActiveResultSets=true",
  "EventStoreConnection": "ConnectTo=tcp://admin:changeit@localhost:1113; HeartBeatTimeout=500",
  "Swagger": {
    "App": {
      "Title": "",
      "Description": "",
      "Version": "v1"
    },
    "Autenticacao": {
      "Name": "JWT Authentication",
      "Description": "Entre apenas com o Bearer token **_only_** \n scopes: api_favo_mel  \n \n Para maiores informações(como autenticação para consumo desta api) acesse a documentação disponível em https://github.com/MaqBr/processo-seletivo-ewave-arquiteto-fev-2021"
